{
  "name": "fleepy.tv",
  "author": "Flleeppyy",
  "description": "My website that I work on.",
  "main": "index.js",
  "website": "https://fleepy.tv",
  "scripts": {
    "start": "yarn sass:build && tsc && PORT=8001 ts-node ./src",
    "dev": "tsc && nodemon --ext 'ts' --watch 'src' --exec 'tsc && NODE_ENV=development PORT=8080 ts-node ./src'",
    "sass:watch": "nodemon --ext 'scss' --watch 'src' --exec 'sass --embed-source-map --update --style=compressed .'",
    "sass:build": "sass --embed-source-map --style=compressed ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flleeppyy/fleepy.tv.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flleeppyy/fleepy.tv/issues"
  },
  "homepage": "https://github.com/flleeppyy/fleepy.tv#readme",
  "dependencies": {
    "@types/jquery": "^3.5.5",
    "@types/node": "^14.14.14",
    "@types/toastr": "^2.1.38",
    "@types/typed.js": "^2.0.9",
    "browserify": "^17.0.0",
    "cheerio": "^1.0.0-rc.3",
    "fastify": "^3.5.0",
    "fastify-static": "^3.2.0",
    "sass": "^1.30.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.0.3",
    "uniq": "^1.0.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "postcss": "^7.0.30",
    "postcss-cli": "^7.1.0",
    "tailwindcss": "^1.3.5"
  }
}
